generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model CarInsuranceQuote {
  id             Int      @id @default(autoincrement()) @map(name: "id")
  ageOfDriver    Int      @map(name: "ageofdriver")
  monthlyPremium Decimal  @map(name: "monthlypremium") @db.Decimal(12, 2)
  yearlyPremium  Decimal  @map(name: "yearlypremium") @db.Decimal(12, 2)
  createdOn      DateTime @map(name: "createdon") @db.Timestamp(6)

  @@map(name: "car_insurance_quote")
}
